{
  "name": "telegram-ai-bot",
  "version": "1.0.8",
  "description": "Telegram group chat AI bot with message summarization and image generation",
  "main": "index.js",
  "scripts": {
    "dev": "netlify dev",
    "build": "tsc --skipLibCheck && echo \"require('./src/server.js');\" > dist/server.js && echo '==> Build output:' && ls -la dist/ && cat dist/server.js",
    "build:skip": "tsc --skipLibCheck --noEmitOnError false",
    "start": "node dist/src/server.js",
    "deploy": "netlify deploy --prod",
    "test": "echo 'No tests configured'",
    "prepare": "echo 'Prepare step completed'",
    "postinstall": "echo '==> Cleaning old dist...' && rm -rf dist"
  },
  "keywords": [
    "telegram",
    "bot",
    "ai",
    "llm",
    "openai",
    "claude",
    "netlify",
    "supabase",
    "grammY"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "@google/generative-ai": "^0.24.1",
    "@netlify/functions": "^2.8.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/express": "^5.0.3",
    "@types/node": "^20.14.0",
    "@types/uuid": "^10.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "grammy": "^1.25.0",
    "replicate": "^1.3.0",
    "sharp": "^0.34.4",
    "typescript": "^5.5.0",
    "uuid": "^13.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
